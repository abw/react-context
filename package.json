{
  "name": "@abw/react-context",
  "version": "2.2.0-alpha4",
  "description": "Utilities for working with React contexts",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/react-context.umd.cjs",
  "module": "./dist/react-context.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/react-context.umd.cjs",
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/react-context.js"
      }
    }
  },
  "author": "Andy Wardley",
  "license": "ISC",
  "keywords": [
    "react",
    "context"
  ],
  "scripts": {
    "dev": "vite --config vite.docs.js",
    "test": "vitest --ui",
    "build": "$npm_execpath build:lib && $npm_execpath build:docs",
    "build:lib": "tsc && vite build",
    "build:docs": "vite build --config vite.docs.js",
    "preview": "vite preview --config vite.docs.js",
    "lint": "eslint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/abw/react-context.git"
  },
  "bugs": {
    "url": "https://github.com/abw/react-context/issues"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@abw/badger-timestamp": "^1.2.0",
    "@abw/badger-utils": "^1.2.2",
    "@abw/badger-website": "^0.3.2",
    "@mdx-js/react": "^3.0.1",
    "@mdx-js/rollup": "^3.0.1",
    "@testing-library/jest-dom": "^6.4.6",
    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.0.11",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitest/ui": "^2.1.8",
    "eslint": "^9.17.0",
    "eslint-plugin-react": "^7.37.3",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "globals": "^15.14.0",
    "jsdom": "^24.1.0",
    "react-router-dom": "^6.10.0",
    "react-syntax-highlighter": "^15.5.0",
    "rehype-mdx-code-props": "^3.0.1",
    "sass": "^1.60.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.19.1",
    "vite": "^5.3.1",
    "vite-jsconfig-paths": "^2.0.1",
    "vite-plugin-dts": "^4.5.0",
    "vite-plugin-svgr": "^4.2.0",
    "vitest": "^2.1.8"
  }
}
